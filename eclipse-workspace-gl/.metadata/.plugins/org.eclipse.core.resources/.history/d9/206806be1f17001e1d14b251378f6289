package practice;

public class SingleTone {
	

	public static void main(String[] args) {
		
		String str="NOON";
		int len= str.length();
		String rev= " ";
		
		for(int i=len-1;i>=0;i--) {
			rev= rev+ str.charAt(i);
		}
		if(str.equals(rev)) {
			System.out.println("Palindrome");
		}
		else{
			System.out.println("not palindrome");
		}
	}

}
